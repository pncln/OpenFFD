FoamFile { version 2.0; format ascii; class volVectorField; object U; }
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type    fixedValue;
        value   uniform (50 0 0);
    }
    outlet
    {
        type    zeroGradient;
        value   uniform (0 0 0);
    }
    airfoil
    {
        type    noSlip;
        value   uniform (0 0 0);
    }
    farfield
    {
        type    symmetryPlane;
        value   uniform (0 0 0);
    }
    frontAndBack
    {
        type    empty;
        value   uniform (0 0 0);
    }
}

