FoamFile { version 2.0; format ascii; class volScalarField; object omega; }
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 2.6;

boundaryField
{
    inlet
    {
        type    fixedValue;
        value   uniform 2.6;
    }
    outlet
    {
        type    zeroGradient;
    }
    airfoil
    {
        type    omegaWallFunction;
        value   uniform 1e6;
    }
    farfield
    {
        type    symmetryPlane;
    }
    frontAndBack
    {
        type    empty;
    }
}

