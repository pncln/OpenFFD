{
    inlet
    {
        type    fixedValue;
        value   uniform 0.375;
    }
    outlet
    {
        type    zeroGradient;
    }
    airfoil
    {
        type    kqRWallFunction;
        value   uniform 1e-10;
    }
    farfield
    {
        type    symmetryPlane;
    }
    frontAndBack
    {
        type    empty;
    }
}

