FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application        simpleFoam;
startFrom        latestTime;
startTime        0;
stopAt        endTime;
endTime        1000.0;
deltaT        1.0;
writeControl        timeStep;
writeInterval        100;
purgeWrite        3;
writeFormat        ascii;
writePrecision        8;
writeCompression        off;
timeFormat        general;
timePrecision        6;
runTimeModifiable        true;

functions
{
    forceCoeffs
    {
        type    forceCoeffs;
        libs    ("libforces.so");
        writeControl    timeStep;
        writeInterval    1;
        patches    (airfoil);
        rho    rhoInf;
        rhoInf    1.225;
        CofR    (0 0 0);
        liftDir    (0 1 0);
        dragDir    (1 0 0);
        pitchAxis    (0 0 1);
        magUInf    50.0;
        lRef    1.0;
        Aref    1.0;
    }
}

